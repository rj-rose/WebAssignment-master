@using WebAssignment.Areas.Identity.Data;
@model List<WebAssignmentUser>
@{
    ViewData["Title"] = "Admin Panel";
    Layout = "~/Views/Shared/_Layout.cshtml";

}
<h1>ADMIN PANEL</h1>
<a asp-action="Main">Admin Statistics</a>
<a asp-action="ViewItems">View All Auctioned Items</a>

<a asp-action="Index">Role Manager</a>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Username</th>
            <th>Email</th>
       

        </tr>
    </thead>
    <tbody>
        @{
            int i = 0;
        }
        @foreach (var role in Model)
        {
            i++;
            <tr>
                <td>@role.UserName</td>
                <td>@role.Email</td>

            </tr>
        }
     
    </tbody>

</table>
@{
    <h10>There are @i accounts in the system</h10>
}